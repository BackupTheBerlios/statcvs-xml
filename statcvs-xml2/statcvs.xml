<statcvs>
  <settings>
    <webRepository>
      http://cvs.berlios.de/cgi-bin/viewcvs.cgi/statcvs-xml/statcvs-xml2/
    </webRepository>
    <authors type="map">
      <setting key="squig">Steffen Pingel</setting>
      <setting key="vanto">Tammo van Lessen</setting>
    </authors>
    
    <authorPics type="map">
      <setting key="squig">squig.png</setting>
      <setting key="vanto">vanto.png</setting>
    </authorPics>
    
	<showImages>true</showImages>
    <projectName>StatCVS-XML</projectName>
    <outputDir>xdocs/statcvs-xml</outputDir>
    <renderer>xdoc</renderer>
    <include>src/de/berlios/statcvs/xml/**/*.java</include>
<!--    <include>xdocs/**/statcvs*.png</include> -->
    <modules type="map">
      <setting key="Report">**/report/*</setting>
      <setting key="Output">**/output/*</setting>
      <setting key="Model">**/model/*</setting>
    </modules>
  </settings>
</statcvs>
